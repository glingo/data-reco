<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
  <class name="net.ar.webonswing.petstore.model.Item" >
    <id name="id" >
      <generator class="native" />
    </id>

    <property name="itemId" />
    <property name="description" length="255" />
    <property name="imagePath" length="255" />
    <property name="listPrice" />
    <property name="unitCost" />

    <many-to-one name="product" class="net.ar.webonswing.petstore.model.Product" cascade="none" outer-join="auto" />
  </class>
</hibernate-mapping>