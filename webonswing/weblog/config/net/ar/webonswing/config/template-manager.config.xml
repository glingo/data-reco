<template-manager>
  <template-cache-active>true</template-cache-active>

  <resources-paths>
    <path>/net/ar/webonswing/resources/html</path>
    <path>/net/ar/webonswing/swing/components</path>
		<path>/weblog/templates</path>
  </resources-paths>
	
  <template-aliases>
		<template>
			<alias>CacheManager</alias>
			<resource-path>/net/ar/webonswing/managers/ui/CacheManager.html</resource-path>
		</template>
		<template>
			<alias>PersistenceManager</alias>
			<resource-path>/net/ar/webonswing/managers/ui/PersistenceManager.html</resource-path>
		</template>
		<template>
			<alias>TemplateManager</alias>
			<resource-path>/net/ar/webonswing/managers/ui/TemplateManager.html</resource-path>
		</template>
		<template>
			<alias>ManagementComponent</alias>
			<resource-path>/net/ar/webonswing/managers/ui/ManagementComponent.html</resource-path>
		</template>
		<template>
			<alias>PageManager</alias>
			<resource-path>/net/ar/webonswing/managers/ui/PageManager.html</resource-path>
		</template>
  </template-aliases>
</template-manager>
